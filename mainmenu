def GetLSProData(page_data,Cookie_Jar,m):
  import xbmcgui
  dialog = xbmcgui.Dialog()  
  opt = dialog.select('Elige una lista:', ['Televisión', 'Radio'])  
  list = ['https://www.tdtchannels.com/lists/tv.m3u', 'https://www.tdtchannels.com/lists/radio.m3u']
  return list[opt] 
]]></expres>
