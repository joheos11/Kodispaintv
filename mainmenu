import re
import HTMLParser
import sys
reload(sys)
sys.setdefaultencoding("utf-8")
def GetLSProData(page_data,Cookie_Jar,m):
 parser = HTMLParser.HTMLParser()
 return re.sub("(&.+?;)", lambda m: parser.unescape(m.group()), page_data)
]]></expres>
