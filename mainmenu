import re
def GetLSProData(page_data,Cookie_Jar,m):
  match = re.compile('group-title="([^"]+)"').findall(page_data)
  seccion_list = []
  for x in match:
    if x not in seccion_list:
      seccion_list.append(x)
  return sorted(seccion_list)
]]></expres>
