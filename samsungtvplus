def GetLSProData(page_data,Cookie_Jar,m):
  import re
  data = re.findall('EXTINF.*?chno="(\d+).*?logo="(.*?)".*?title="(.*?)",(.*)\n(.*)',page_data)
  data.sort()
  return data
]]></expres>
