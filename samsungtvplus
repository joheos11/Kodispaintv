<expres><![CDATA[#$pyFunction
def GetLSProData(page_data,Cookie_Jar,m):
  import re
  titulo = '[makelist.param1]'.replace("+","\+")
  texto = 'EXTINF.*?chno="(\d+).*?logo="(.*?)".*?title="' + titulo + '",(.*)\n(.*)'
  data = re.findall(texto, page_data)
  data.sort()
  return data
]]></expres>
